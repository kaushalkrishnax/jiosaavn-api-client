{
  "name": "jiosaavn-api-client",
  "version": "1.0.2",
  "description": "Pure-JS client for JioSaavn's native API. Works on Node, Bun, Cloudflare Workers, Vercel Edge, and Deno.",
  "type": "module",
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.ts"
    },
    "./utils": {
      "types": "./utils.ts",
      "import": "./utils.ts"
    },
    "./fetch": {
      "types": "./fetch.ts",
      "import": "./fetch.ts"
    }
  },
  "files": [
    "*.ts",
    "*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "bun run test.js"
  },
  "keywords": [
    "jiosaavn",
    "saavn",
    "music",
    "api",
    "cloudflare-workers",
    "vercel-edge",
    "streaming",
    "download",
    "songs",
    "albums",
    "playlists"
  ],
  "author": "Kaushal Krishna <kaushalkrishnax@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaushalkrishnax/jiosaavn-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/kaushalkrishnax/jiosaavn-api-client/issues"
  },
  "homepage": "https://github.com/kaushalkrishnax/jiosaavn-api-client#readme",
  "dependencies": {
    "node-forge": "^1.3.3"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
